<div class="container mt-4">
  <h1>Patients</h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Medical History</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @patients.each do |patient| %>
        <tr>
          <td><%= patient.name %></td>
          <td><%= patient.medical_history %></td>
          <td>
            <%= link_to 'Edit', edit_patient_path(patient), class: 'btn btn-primary' %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
